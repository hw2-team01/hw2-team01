  	<dependency>
  		<groupId>
  			edu.washington.cs.knowitall.stanford-corenlp
  		</groupId>
  		<artifactId>stanford-postag-models</artifactId>
  		<version>1.3.2</version>
  	</dependency>
  	<dependency>
